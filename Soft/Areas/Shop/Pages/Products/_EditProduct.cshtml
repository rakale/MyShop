 @using Abc.Pages.Common.Extensions;
@model Abc.Pages.Shop.ProductsPage;

<body>
    @Html.Editor(m => m.Item.Id)
    @Html.Editor(m => m.Item.Code)
    @Html.Editor(m => m.Item.Name)
    @Html.Editor(m => m.Item.Description)
    @Html.Editor(m => m.Item.Price)
    @Html.Editor(m => m.Item.PictureUri)
    @Html.DropDown(m => m.Item.CatalogTypeId, Model.Catalogs)
    @Html.DropDown(m => m.Item.CatalogBrandId, Model.Brands)
    @Html.Editor(m => m.Item.From)
    @Html.Editor(m => m.Item.To)
</body>
